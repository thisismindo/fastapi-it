[mysqld]
server-id=1
log-bin=mysql-bin
binlog_cache_size=1M
binlog_format=ROW
character-set-server=utf8mb4
collation-server=utf8mb4_unicode_ci
init_connect='SET NAMES utf8mb4'
general_log_file=/var/log/mysql.log
general_log=1

[client]
default-character-set=utf8mb4

[mysql]
default-character-set=utf8mb4
